{
  "name": "workerman/webman",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "https://www.workerman.net",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "walkor",
      "email": "walkor@workerman.net",
      "homepage": "https://www.workerman.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "walkor@workerman.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "https://wenda.workerman.net/",
    "wiki": "https://workerman.net/doc/webman",
    "source": "https://github.com/walkor/webman"
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-pdo": "*",
    "ext-iconv": "*",
    "ext-bcmath": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-redis": "*",
    "ext-zip": "*",
    "workerman/webman-framework": "^1.4.3",
    "monolog/monolog": "^2.0",
    "workerman/crontab": "^1.0",
    "illuminate/container": "^9.48",
    "illuminate/database": "^9.48",
    "illuminate/pagination": "^9.48",
    "illuminate/events": "^9.48",
    "symfony/var-dumper": "^6.0",
    "tinywan/captcha": "^0.0.2",
    "illuminate/redis": "^9.48",
    "predis/predis": "^2.1",
    "vlucas/phpdotenv": "^5.5",
    "webman-tech/laravel-validation": "^1.1",
    "ext-gd": "*",
    "webman/log": "^1.1",
    "shopwwi/webman-auth": "^1.0",
    "webman/gateway-worker": "^1.0",
    "webman/redis-queue": "^1.2",
    "yzh52521/webman-task": "^1.35",
    "webman-tech/laravel-cache": "^1.1",
    "webman-tech/laravel-http-client": "^1.1"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app",
      "App\\": "./app",
      "app\\View\\Components\\": "./app/view/components"
    },
    "files": [
      "./support/helpers.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  }
}
